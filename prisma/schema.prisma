generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model toy_table {
  id      Int     @id @default(autoincrement())
  name    String?
  surname String?
  pet     String?
  married Int
}

model toy_table2 {
  id      Int     @id @default(autoincrement())
  name    String?
  surname String?
  pet     String?
  married Bytes?
}
